Declaration(Class(:Outbox))
Declaration(Class(:Following))
Declaration(Class(:Followers))
Declaration(Class(:Liked))
Declaration(Class(:Streams))

Declaration(Class(:Correspondents))
SubClassOf(:Correspondents as:Collection)

SubClassOf(:Outbox :Correspondents)
SubClassOf(:Following :Correspondents)
SubClassOf(:Followers :Correspondents)
SubClassOf(:Liked :Correspondents)
SubClassOf(:Streams :Correspondents)
SubClassOf(:Inbox ObjectUnionOf(st:Inbox :Correspondents))


